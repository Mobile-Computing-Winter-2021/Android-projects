<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E8D5DC"
    tools:context=".Fragmentlocate">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="413dp"
        android:layout_height="318dp"
        android:layout_marginTop="4dp"
        android:background="#D69CB0"
        android:src="@drawable/location1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="365dp"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="344dp"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="WHERE AM 1 WITH EUCLEDIAN?"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="242dp"
        android:layout_height="83dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="452dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="366dp"
        android:layout_height="39dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="393dp"
        android:text="WHERE AM I WITH KNN?"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="246dp"
        android:layout_height="83dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="544dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>